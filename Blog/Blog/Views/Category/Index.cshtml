@model List<Category>

<h1>Category List</h1>
<table class="table table-bordered table-striped">
    <thead>
        <tr>
            <th>
                Name
            </th>
            <th>
                Description
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(o => o.DisplayOrder)) {
        <tr>
            <td>@obj.Name</td>
            <td>@obj.CategoryDescription</td>
        </tr>
        }
    </tbody>
</table>